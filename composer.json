{
    "name": "webjump/magento2-module-braspagpagador",
    "license": "MIT",
    "type": "magento2-module",
    "description": "Braspag Pagador integration for Magento 2",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "~5.6.0|~7.0.0",
        "magento/module-payment": "100.1.*",
        "magento/module-sales": "100.1.*",
        "magento/module-config": "100.1.*",
        "magento/module-store": "100.1.*",
        "magento/module-quote": "100.1.*",
        "magento/module-checkout": "100.1.*",
        "magento/framework": "100.1.*",
        "zendframework/zend-stdlib": "~2.4.6",
        "zendframework/zend-escaper":"~2.4.6",
        "webjump/braspagpagador-sdk-adapter":"dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.0",
        "squizlabs/php_codesniffer": "1.5.3",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.2.2",
        "sjparkinson/static-review": "~4.1",
        "fabpot/php-cs-fixer": "~1.2",
        "lusitanian/oauth": "~0.3 <=0.7.0",
        "sebastian/phpcpd": "2.0.0",
        "phpunit/phpunit-selenium": ">=1.2"
    },
    "authors":[
        {
            "name":"Webjump Core Team",
            "email":"dev@webjump.com.br"
        }
    ],
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Webjump\\BraspagPagador\\": ""
        }
    }
}